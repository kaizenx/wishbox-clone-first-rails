<%= simple_form_for(@contact_u) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :FirstName %>
    <%= f.input :LastName %>
    <%= f.input :Email %>
    <%= f.input :Comment %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
