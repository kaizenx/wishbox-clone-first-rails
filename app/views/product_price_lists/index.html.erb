<h1>Listing product_price_lists</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Dayofyear</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @product_price_lists.each do |product_price_list| %>
      <tr>
        <td><%= product_price_list.Price %></td>
        <td><%= product_price_list.DayOfYear %></td>
        <td><%= link_to 'Show', product_price_list %></td>
        <td><%= link_to 'Edit', edit_product_price_list_path(product_price_list) %></td>
        <td><%= link_to 'Destroy', product_price_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

